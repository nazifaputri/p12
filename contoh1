#include <iostream>
using namespace std;

void main()
{
	//mendefinisikan array beserta isinya
	char arraychar1[5]={'A','B','C'};  
	int arrayku[5]={23,45,20,
			17,24};
	int arraybilangan[5];

	//menampilkan isi array pada posisi kedua
	cout << "isi array ke 2 = "<<arraychar1[1]<<endl;
	cout<<endl;

	//mengisi array pada posisi ke 4 
	arraychar1[3]='D';

	//menampilkan semua isi array
	for (int i=0; i<5; i++)
	{
		cout << "isi Arraychar1 index " <<i<< " = "<<arraychar1[i]<<endl;
	}

	//menampilkan arrayku
	cout<<endl;
	cout<<"Arrayku adalah : ";
	for(int i=0; i<5; i++)
	{
		cout<<" "<<arrayku[i];
	}
	cout<<endl;

	
}
